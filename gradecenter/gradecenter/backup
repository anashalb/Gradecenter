CREATE TABLE "auth_permission" (
    "id" integer NOT NULL PRIMARY KEY,
    "name" varchar(50) NOT NULL,
    "content_type_id" integer NOT NULL,
    "codename" varchar(100) NOT NULL,
    UNIQUE ("content_type_id", "codename")
);
;
CREATE TABLE "auth_group_permissions" (
    "id" integer NOT NULL PRIMARY KEY,
    "group_id" integer NOT NULL,
    "permission_id" integer NOT NULL REFERENCES "auth_permission" ("id"),
    UNIQUE ("group_id", "permission_id")
);
;
CREATE TABLE "auth_group" (
    "id" integer NOT NULL PRIMARY KEY,
    "name" varchar(80) NOT NULL UNIQUE
);
;
CREATE TABLE "auth_user_user_permissions" (
    "id" integer NOT NULL PRIMARY KEY,
    "user_id" integer NOT NULL,
    "permission_id" integer NOT NULL REFERENCES "auth_permission" ("id"),
    UNIQUE ("user_id", "permission_id")
);
;
CREATE TABLE "auth_user_groups" (
    "id" integer NOT NULL PRIMARY KEY,
    "user_id" integer NOT NULL,
    "group_id" integer NOT NULL REFERENCES "auth_group" ("id"),
    UNIQUE ("user_id", "group_id")
);
;
CREATE TABLE "auth_user" (
    "id" integer NOT NULL PRIMARY KEY,
    "username" varchar(30) NOT NULL UNIQUE,
    "first_name" varchar(30) NOT NULL,
    "last_name" varchar(30) NOT NULL,
    "email" varchar(75) NOT NULL,
    "password" varchar(128) NOT NULL,
    "is_staff" bool NOT NULL,
    "is_active" bool NOT NULL,
    "is_superuser" bool NOT NULL,
    "last_login" datetime NOT NULL,
    "date_joined" datetime NOT NULL
);
;
CREATE TABLE "auth_message" (
    "id" integer NOT NULL PRIMARY KEY,
    "user_id" integer NOT NULL REFERENCES "auth_user" ("id"),
    "message" text NOT NULL
);
CREATE TABLE "django_content_type" (
    "id" integer NOT NULL PRIMARY KEY,
    "name" varchar(100) NOT NULL,
    "app_label" varchar(100) NOT NULL,
    "model" varchar(100) NOT NULL,
    UNIQUE ("app_label", "model")
);
;
CREATE TABLE "django_session" (
    "session_key" varchar(40) NOT NULL PRIMARY KEY,
    "session_data" text NOT NULL,
    "expire_date" datetime NOT NULL
);
;
CREATE TABLE "django_site" (
    "id" integer NOT NULL PRIMARY KEY,
    "domain" varchar(100) NOT NULL,
    "name" varchar(50) NOT NULL
);
CREATE TABLE "GradeCenter_professor" (
    "id" integer NOT NULL PRIMARY KEY,
    "username" varchar(200) NOT NULL,
    "andrewid" varchar(200) NOT NULL
);
CREATE TABLE "GradeCenter_student" (
    "id" integer NOT NULL PRIMARY KEY,
    "username" varchar(200) NOT NULL,
    "andrewid" varchar(200) NOT NULL
);
CREATE TABLE "GradeCenter_course_instance" (
    "id" integer NOT NULL PRIMARY KEY,
    "professor_id" integer NOT NULL REFERENCES "GradeCenter_professor" ("id"),
    "courseid_id" integer NOT NULL REFERENCES "GradeCenter_course" ("id"),
    "year" integer NOT NULL
);
CREATE TABLE "GradeCenter_enroll" (
    "id" integer NOT NULL PRIMARY KEY,
    "courseid_id" integer NOT NULL REFERENCES "GradeCenter_course_instance" ("id"),
    "studentid_id" integer NOT NULL REFERENCES "GradeCenter_student" ("id")
);
CREATE TABLE "GradeCenter_grade" (
    "id" integer NOT NULL PRIMARY KEY,
    "workid_id" integer NOT NULL REFERENCES "GradeCenter_work" ("id"),
    "student_id" integer NOT NULL REFERENCES "GradeCenter_student" ("id"),
    "grade" integer NOT NULL
);
CREATE INDEX "auth_permission_e4470c6e" ON "auth_permission" ("content_type_id");
CREATE INDEX "auth_group_permissions_bda51c3c" ON "auth_group_permissions" ("group_id");
CREATE INDEX "auth_group_permissions_1e014c8f" ON "auth_group_permissions" ("permission_id");
CREATE INDEX "auth_user_user_permissions_fbfc09f1" ON "auth_user_user_permissions" ("user_id");
CREATE INDEX "auth_user_user_permissions_1e014c8f" ON "auth_user_user_permissions" ("permission_id");
CREATE INDEX "auth_user_groups_fbfc09f1" ON "auth_user_groups" ("user_id");
CREATE INDEX "auth_user_groups_bda51c3c" ON "auth_user_groups" ("group_id");
CREATE INDEX "auth_message_fbfc09f1" ON "auth_message" ("user_id");
CREATE INDEX "django_session_c25c2c28" ON "django_session" ("expire_date");
CREATE INDEX "GradeCenter_course_instance_c8a2bdb2" ON "GradeCenter_course_instance" ("professor_id");
CREATE INDEX "GradeCenter_course_instance_f34f10c4" ON "GradeCenter_course_instance" ("courseid_id");
CREATE INDEX "GradeCenter_enroll_f34f10c4" ON "GradeCenter_enroll" ("courseid_id");
CREATE INDEX "GradeCenter_enroll_13c831a5" ON "GradeCenter_enroll" ("studentid_id");
CREATE INDEX "GradeCenter_grade_84b634e2" ON "GradeCenter_grade" ("workid_id");
CREATE INDEX "GradeCenter_grade_42ff452e" ON "GradeCenter_grade" ("student_id");
CREATE TABLE "GradeCenter_final_grade" (
    "id" integer NOT NULL PRIMARY KEY,
    "courseinstance_id" integer NOT NULL REFERENCES "GradeCenter_course_instance" ("id"),
    "student_id" integer NOT NULL REFERENCES "GradeCenter_student" ("id"),
    "grade" varchar(2) NOT NULL
);
CREATE INDEX "GradeCenter_final_grade_dc144620" ON "GradeCenter_final_grade" ("courseinstance_id");
CREATE INDEX "GradeCenter_final_grade_42ff452e" ON "GradeCenter_final_grade" ("student_id");
CREATE TABLE "GradeCenter_course" (
    "id" integer NOT NULL PRIMARY KEY,
    "number" integer NOT NULL,
    "name" varchar(200) NOT NULL,
    "units" integer NOT NULL,
    "description" varchar(9999) NOT NULL
);
CREATE TABLE gradecenter_work (
    "id" INTEGER NOT NULL,
    "title" VARCHAR(200) NOT NULL,
    "courseid_id" INTEGER NOT NULL,
    "description" VARCHAR(9999) NOT NULL,
    "weight" INTEGER NOT NULL,
    "duedate" DATE NOT NULL,
    "duetime" TIME NOT NULL
, "maxpoints" INTEGER   DEFAULT ('NULL'));
